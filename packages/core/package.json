{
  "name": "@templarios/core",
  "version": "0.0.1",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/core/core.esm.js",
  "files": [
    "dist/",
    "loader/",
    "public/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 3669",
    "build-storybook": "storybook build",
    "tp:watch:sass": "sass src/utils/styles:public/css src/styles:public/css src/components/ionic:public/css --watch",
    "tp:watch:stencil:build": "stencil build --watch",
    "tp:start:storybook": "storybook dev -p 3669",
    "tp:start": "npm-run-all --parallel tp:watch:sass tp:watch:stencil:build tp:start:storybook",
    "tp:build:sass": "sass src/utils/styles:public/css src/styles:public/css src/components/ionic:public/css",
    "tp:build:stencil": "stencil build --docs",
    "tp:build:storybook": "storybook build",
    "tp:build": "npm-run-all tp:build:sass tp:build:stencil"
  },
  "dependencies": {
    "@ionic/core": "^7.5.0",
    "@stencil/core": "^4.0.0",
    "ionicons": "^7.2.1"
  },
  "devDependencies": {
    "@stencil/sass": "^3.0.6",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/blocks": "^7.4.6",
    "@storybook/jest": "^0.2.3",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/web-components": "^7.4.6",
    "@storybook/web-components-vite": "^7.4.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "lit": "^2.8.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "21.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.68.0",
    "storybook": "^7.4.6",
    "storybook-addon-themes": "^6.1.0"
  },
